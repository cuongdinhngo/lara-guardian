{
    "name": "cuongnd88/lara-guardian",
    "description": "Laravel Guardian makes it easy to perform permission",
    "keywords": [
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "guardian",
        "security"
    ],
    "homepage": "https://github.com/cuongnd88/lara-guardian",
    "license": "MIT",
    "authors": [
        {
            "name": "Ngo Dinh Cuong",
            "email": "dinhcuongngo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "cuongnd88/lara-query-kit": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Cuongnd88\\LaraGuardian\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cuongnd88\\LaraGuardian\\LaraGuardianServiceProvider"
            ]
        }
    }
}
